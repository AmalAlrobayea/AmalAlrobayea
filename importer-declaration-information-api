definitions:
  DefaultFailureResonse:
    type: object
    additionalProperties: false
    properties:
      httpCode:
        type: string
      httpMessage:
        type: string
      moreInformation:
        type: string
  DeclarationsItem:
    type: object
    additionalProperties: false
    properties:
      importerNumber:
        type: number
      declarationNumber:
        type: number
      shipmentType:
        type: string
      licenseNumber:
        type: string
  DeclarationDetailsItem:
    type: object
    additionalProperties: false
    properties:
      portName:
        type: string
      importerNumber:
        type: number
      arImporterName:
        type: string
      eta:
        type: string
        format: date
      shipmentNumber:
        type: string
      declarationNumber:
        type: number
      declarationDate:
        type: string
        format: date
      declarationTypeDesc:
        type: string
      shipmentType:
        type: string
      statusDesc:
        type: string
      licenseNumber:
        type: string
      brokerName:
        type: string
  DeclarationsList:
    type: array
    items:
      $ref: '#/definitions/DeclarationsItem'
  DeclarationDetails:
    type: array
    items:
      $ref: '#/definitions/DeclarationDetailsItem'
paths:
  '/{importer_number}/declarations/{declaration_number}':
    parameters:
      - name: importer_number
        in: path
        required: true
        type: number
      - name: declaration_number
        in: path
        required: true
        type: number
    get:
      responses:
        '200':
          description: success
          schema:
            $ref: '#/definitions/DeclarationDetails'
        '400':
          description: bad request
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '401':
          description: unauthorized
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '404':
          description: No resources match requested URI
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '405':
          description: method not allowed
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '500':
          description: internal error
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
      operationId: Details of specific declaration for an importer
      consumes: []
      produces:
        - application/json
  '/{importer_number}/declarations':
    parameters:
      - name: importer_number
        in: path
        required: true
        type: number
    get:
      responses:
        '200':
          description: success
          schema:
            $ref: '#/definitions/DeclarationsList'
        '400':
          description: bad request
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '401':
          description: unauthorized
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '404':
          description: No resources match requested URI
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '405':
          description: method not allowed
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '500':
          description: internal error
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
      operationId: List of declataions and shipment types
      consumes: []
      produces:
        - application/json
