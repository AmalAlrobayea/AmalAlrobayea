definitions:
  DefaultFailureResonse:
    type: object
    additionalProperties: false
    properties:
      httpCode:
        type: string
      httpMessage:
        type: string
      moreInformation:
        type: string
  OrigDutyDeclarationsItem:
    type: object
    additionalProperties: false
    properties:
      importerNumber:
        type: number
      declarationNumber:
        type: string
  OrigDutyDeclarationsList:
    type: array
    items:
      $ref: '#/definitions/OrigDutyDeclarationsItem'
  OrigDutyDeclarationDetailsItem:
    type: object
    additionalProperties: false
    properties:
      portName:
        type: string
      declarationNumber:
        type: number
      declarationDate:
        type: string
        format: date
      brokerName:
        type: string
      statusDesc:
        type: string
      cifValue:
        type: string
      origDuty:
        type: string
      demurrageFee:
        type: string
  OrigDutyDeclarationDetails:
    type: array
    items:
      $ref: '#/definitions/OrigDutyDeclarationDetailsItem'
paths:
  '/{importer_number}/declarations/{declaration_number}/originalduty':
    parameters:
      - name: importer_number
        in: path
        required: true
        type: number
      - name: declaration_number
        in: path
        required: true
        type: number
    get:
      responses:
        '200':
          description: success
          schema:
            $ref: '#/definitions/OrigDutyDeclarationDetails'
        '400':
          description: bad request
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '401':
          description: unauthorized
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '404':
          description: No resources match requested URI
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '405':
          description: method not allowed
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '500':
          description: internal error
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
      operationId: Details of original duty of a declaration of an importer
      consumes: []
      produces:
        - application/json
  '/{importer_number}/declarations/originalduty':
    parameters:
      - name: importer_number
        in: path
        required: true
        type: number
    get:
      responses:
        '200':
          description: success
          schema:
            $ref: '#/definitions/OrigDutyDeclarationsList'
        '400':
          description: bad request
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '401':
          description: unauthorized
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '404':
          description: No resources match requested URI
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '405':
          description: method not allowed
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
        '500':
          description: internal error
          schema:
            $ref: '#/definitions/DefaultFailureResonse'
      operationId: List of declarations related to original duty of an importer
      consumes: []
      produces:
        - application/json
