    scopes:
      Broker: Broker
      Fasah: Fasah
      GATEOPERATOR: GATEOPERATOR
      IMPRT: Importer
      PORT_CONTRACTOR: PORT_CONTRACTOR
      SUPERBROKER: SUPERBROKER
      Trader: Trader
  clientSecret:
    type: apiKey
