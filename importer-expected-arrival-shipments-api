definitions:
  ShipmentDetails:
    type: array
    items:
      $ref: '#/definitions/ShipmentDetailsItem'
  DefaultFailureResonse:
    type: object
    additionalProperties: false
    properties:
      httpCode:
        type: string
      httpMessage:
        type: string
      moreInformation:
        type: string
  ShipmentDetailsItem:
    type: object
    additionalProperties: false
    properties:
      portName:
        type: string
      importerNumber:
        type: number
      arImporterName:
        type: string
      vesselName:
        type: string
      eta:
        type: string
        format: date
      shipmentNumber:
        type: string
      shipmentType:
        type: string
      shippingLine:
        type: string
  ShipmentListItem:
    type: object
    additionalProperties: false
    properties:
      importerNumber:
        type: number
      shipmentNumber:
        type: string
  DistinctShipmentList:
    type: array
    items:
      $ref: '#/definitions/ShipmentListItem'
