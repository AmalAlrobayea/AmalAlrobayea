swagger: '2.0'
info:
  title: Importer Expected Arrival Shipments API
  x-ibm-name: importer-expected-arrival-shipments-api
  version: 1.0.0
  description: 'This API provides a list of expected shipments for specific time period and related to a particular Importer and also provides the details of expected shipment. E.g. Vessel Name, ETA, ...etc.'
schemes:
  - https
basePath: /tabadul/fasahsb/api/v1/analytics/ieas/importer
